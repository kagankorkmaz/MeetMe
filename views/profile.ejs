                    <p><%= user.totalMeetings %></p>
                  </div>
                </div>
                <div class="row">
                  <div class="col-md-12">
                    <label>Your Bio</label><br />
                    <p><%= user.bio %></p>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
