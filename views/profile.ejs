                <div style="margin-top: 20px;"></div>
                <li><a href="<%= user.twitter %>"></a><span class="icon-twitter"></span></a></li>
                <li><a href="<%= user.linkedin %>"></a><span class="icon-linkedin"></span></a></li>
                <li><a href="<%= user.instangram %>"></a><span class="icon-instagram"></span></a></li>
              </ul>
            </div>
          </div>
